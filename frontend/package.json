{
  "name": "check-in-daikoku",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "openApi:cp": "cp ../backend/openApi/swagger-spec.yaml ./src/api/swagger-spec.yaml",
    "openApi:generate": "npx @openapitools/openapi-generator-cli generate -i ./src/api/swagger-spec.yaml -g typescript-axios -o ./src/api/generated",
    "openApi": "yarn openApi:cp && yarn openApi:generate"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.3.1",
    "@mui/material": "^6.2.1",
    "@mui/styled-engine": "^6.1.3",
    "@openapitools/openapi-generator-cli": "^2.14.0",
    "@turf/turf": "^7.1.0",
    "@types/node": "^22.10.5",
    "@types/react-helmet": "^6.1.11",
    "@types/turf": "^3.5.32",
    "axios": "^1.7.7",
    "chart.js": "^4.4.6",
    "chartjs-plugin-datalabels": "^2.2.0",
    "mui": "^0.0.1",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-markdown": "^9.0.3",
    "react-router-dom": "^6.27.0",
    "sass-embedded": "^1.82.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8"
  }
}
